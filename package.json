{
   "name": "ngx-chronology",
   "version": "1.0.0",
   "scripts": {
      "ng": "ng",
      "start": "ng serve",
      "build": "echo \"Error: Please specify which project you want to build via npm run build{:lib|:demo}\" && exit 1",
      "build:lib": "ng build ngx-chronology",
      "build:demo": "ng build ngx-chronology-demo",
      "build:prod": "npm run build:lib:prod && npm run build:demo:prod",
      "build:lib:prod": "ng build ngx-chronology --prod",
      "build:demo:prod": "ng build ngx-chronology-demo --prod --base-href /ngx-chronology/",
      "test": "npm run test:lib && npm run test:demo",
      "test:lib": "ng test ngx-chronology",
      "test:demo": "ng test ngx-chronology-demo",
      "lint": "ng lint",
      "format": "prettier --write \"**/*.*\""
   },
   "author": {
      "email": "JReed129@gmail.com",
      "name": "Jeremy Reed",
      "url": "http://jr33d.github.io"
   },
   "repository": {
      "type": "GitHub",
      "url": "https://github.com/jr33d/ngx-chronology"
   },
   "bugs": {
      "url": "https://github.com/jr33d/ngx-chronology/issues"
   },
   "keywords": [
      "Angular",
      "Timeline",
      "Chronology",
      "Component",
      "ngx"
   ],
   "license": "The Unlicense",
   "dependencies": {
      "@angular/common": "^6.0.2",
